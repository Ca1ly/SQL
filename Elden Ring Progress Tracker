ALTER TABLE EldenRing ADD death Integer default "unknown";

UPDATE EldenRing SET death = 1 WHERE id = 1;

UPDATE EldenRing SET death = 0 WHERE id = 2;

UPDATE EldenRing SET death = 10 WHERE id = 3;

